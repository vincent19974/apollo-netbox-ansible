[global_tags]
    datacenter = "DEMO_LAB"
    collector = "TELEGRAF01.LAB"

[agent]
    interval = "60s"
    round_interval = true
    metric_batch_size = 1000
    metric_buffer_limit = 10000
    collection_jitter = "0s"
    flush_interval = "10s"
    flush_jitter = "0s"
    precision = ""


[[outputs.influxdb]]
    urls = ["http://influxdb:8086"]
    database = "telegraf"
    timeout = "5s"

[[inputs.ping]]
    interval = "5s"
    urls = ["192.168.1.251"]
    count = 4
    ping_interval = 1.0
    timeout = 2.0